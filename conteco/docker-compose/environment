#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export CONTECO_DC_GLOBAL_SERVICES='controlplane.conteco'
export CONTECO_DC_GLOBAL='$CONTECO_DC_GLOBAL_SERVICE_NAME,$CONTECO_DC_GLOBAL_CONTROLPLANE_CONTECO_NAME,$CONTECO_DC_CONTROLPLANE_CONTECO_ENVIRONMENT,$CONTECO_DC_CONTROLPLANE_CONTECO_COMMAND,$CONTECO_DC_CONTROLPLANE_CONTECO_VOLUMES,$CONTECO_DC_CONTROLPLANE_CONTECO_NETWORKS,$CONTECO_DC_CONTROLPLANE_CONTECO_PORTS,$CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_MODE,$CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_REPLICAS,$CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_LIMITS_CPUS,$CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_RESERVATIONS_CPUS,$CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_LIMITS_MEMORY,$CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_RESERVATIONS_MEMORY'
# END CONFIGURATION

# START GLOBAL SETTINGS
export CONTECO_DC_GLOBAL_SERVICE_NAME="${CONTECO_TYPE}"
export CONTECO_DC_GLOBAL_IMAGE_NAME="${CONTECO_TYPE}.${CONTECO_NAME}:${CONTECO_TAG}"
export CONTECO_DC_CONTROLPLANE_CONTECO_ENVIRONMENT=
export CONTECO_DC_CONTROLPLANE_CONTECO_COMMAND="'usleep 500000000'"
export CONTECO_DC_CONTROLPLANE_CONTECO_PORTS=
export CONTECO_DC_CONTROLPLANE_CONTECO_VOLUMES=
export CONTECO_DC_CONTROLPLANE_CONTECO_NETWORKS=
export CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_LABELS=
export CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_MODE="replicated"
export CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_REPLICAS="1"
export CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_LIMITS_CPUS="0.75"
export CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_RESERVATIONS_CPUS="0.0"
export CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END GLOBAL SETTINGS

# START SETTINGS controlplane.conteco
# END SETTINGS controlplane.conteco

# START TOPLEVEL SETTINGS
export CONTECO_DC_GLOBAL_VOLUMES=
export CONTECO_DC_GLOBAL_NETWORKS=
# END TOPLEVEL SETTINGS
