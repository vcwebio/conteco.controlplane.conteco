#!/usr/bin/env bash
if [[ "$1" == "for-each-node" ]] ; then
(
	shift
    . executionplane-invoke "$0 $@"
	command=$1
	shift
	executionplane-error "Method not implemented: ${CONTECO_NAME} $(command $0)."
    executionplane-complete
)
else
    export PATH="$CONTECO_EXECUTIONPLANE_APIINTERNALPATH_CONTECO"
	for-each $(basename $0) $@
fi
